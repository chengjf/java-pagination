<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>PageServlet</servlet-name>
		<servlet-class>com.chengjf.page.servlet.PageServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>PageServlet</servlet-name>
		<url-pattern>/page</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>AnotherPageServlet</servlet-name>
		<servlet-class>com.chengjf.page.servlet.AnotherPageServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>AnotherPageServlet</servlet-name>
		<url-pattern>/anotherpage</url-pattern>
	</servlet-mapping>

	<session-config>
		<session-timeout>30</session-timeout>
	</session-config>
	
	<welcome-file-list>
		<welcome-file>index.jsp</welcome-file>
	</welcome-file-list>
</web-app>
